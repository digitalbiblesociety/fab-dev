{
	"name": "fab",
	"version": "0.0.1",
	"scripts": {
		"dev": "concurrently -n kit,i18n \"pnpm run watch:translations\" \"pnpm run dev:kit\"",
		"dev:kit": "svelte-kit dev",
		"build": "pnpm run build:translations && svelte-kit build",
		"rebuild": "pnpm run clean && pnpm run build",
		"clean": "rm -rf .svelte-kit && rm -rf build && rm src/generated/translations.js",
		"publish": "pnpm run rebuild && cd ./build && echo \"dbs.org\" >| CNAME && git init && git add . && git commit -m \"auto-push\" && git branch -M main && git remote add origin git@github.com:digitalbiblesociety/fab-prod.git && git push -f -u origin main",
		"publish-dev": "pnpm run rebuild && cd ./build && echo \"dev.dbs.org\" >| CNAME && git init && git add . && git commit -m \"auto-push\" && git branch -M main && git remote add origin git@github.com:digitalbiblesociety/fab-dev.git && git push -f -u origin main",
		"preview": "svelte-kit preview",
		"test": "playwright test",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"build:translations": "messageformat src/translations/ --outfile=src/generated/translations.js",
		"watch:translations": "pnpm run build:translations && onchange 'src/translations/**' -- pnpm run build:translations"
	},
	"devDependencies": {
		"@antfu/ni": "^0.9.3",
		"@beyonk/svelte-google-analytics": "^2.2.0",
		"@messageformat/cli": "^3.0.1",
		"@messageformat/core": "^3.0.0",
		"@stripe/stripe-js": "^1.19.1",
		"@sveltejs/adapter-node": "next",
		"@sveltejs/adapter-static": "next",
		"@sveltejs/kit": "next",
		"@tailwindcss/aspect-ratio": "^0.4.0",
		"@tailwindcss/forms": "^0.4.0",
		"@tailwindcss/line-clamp": "^0.3.0",
		"@tailwindcss/typography": "^0.5.0",
		"@typescript-eslint/eslint-plugin": "^4.19.0",
		"@typescript-eslint/parser": "^4.19.0",
		"@playwright/test": "^1.21.0",
		"autoprefixer": "^10.4.0",
		"concurrently": "^6.2.1",
		"gh-pages": "^3.2.3",
		"onchange": "^7.1.0",
		"postcss": "^8.4.5",
		"postcss-load-config": "^3.1.0",
		"prettier": "^2.5.1",
		"prettier-plugin-tailwindcss": "^0.1.7",
		"prisma": "^3.1.1",
		"svelte": "^3.46.1",
		"svelte-check": "^2.0.0",
		"svelte-preprocess": "^4.9.0",
		"svelte-range-slider-pips": "^2.0.0",
		"svelte-stripe-js": "^0.0.8",
		"tailwindcss": "^3.0.6",
		"tslib": "^2.0.0",
		"typescript": "^4.4.0",
		"vite": "^2.8.5"
	},
	"type": "module",
	"engines": {
		"node": ">= 12.17.0"
	},
	"dependencies": {
		"@messageformat/runtime": "^3.0.0",
		"@prisma/client": "^3.1.1",
		"@rgossiaux/svelte-headlessui": "^1.0.0-beta.8",
		"@sveltejs/pancake": "^0.0.18",
		"d3": "^7.2.1",
		"d3-geo": "^3.0.1",
		"d3-zoom": "^3.0.0",
		"fuse.js": "^6.4.6",
		"google-auth-library": "^7.10.4",
		"graphql": "^16.0.1",
		"knex": "^0.95.11",
		"leaflet": "^1.8.0",
		"mysql": "^2.18.1",
		"qrcode-svg": "^1.1.0",
		"query-store": "^1.1.2",
		"svelte-forms": "^2.0.7",
		"svelte-heatmap": "^1.0.2",
		"svelte-i18n": "^3.3.10",
		"svelte-select": "^4.4.7",
		"svelte-tiny-linked-charts": "^1.0.6",
		"topojson": "^3.0.2",
		"svelte-bible-wiki": "file:../svelte-packages/svelte-bible-wiki",
		"svelte-simple-datatables": "file:../svelte-packages/svelte-simple-datatables",
		"sverocons": "^0.0.2"
	}
}
